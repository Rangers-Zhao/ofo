/* pages/wellat/index.wxss */
.container{
  width: 100vw;
  height:100vh;
  background-color: #f2f2f2;
}
.overage{
  padding: 40rpx 0;
  background-color: #fff;
  text-align: center;
}
.overage-header{
  color: #999;
  font-size: 28rpx;
}
.overage-money{
  font-size: 100rpx;
  font-weight: 500;
}
.overage-desc{
  padding: 5rpx 10rpx;
  border: 1px solid #999;
  border-radius: 30rpx;
  color: #999;
  font-size: 28rpx;
}
.btn-charge{
  width: 90%;
  margin: 40rpx auto 30rpx;
  background: #b9dd08;
}
.tapbar{
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  background: #fff;
}
.deposite{
  margin-top:3rpx;
}
.about{
  margin-top: 20rpx;
}